{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "code",
    "msg",
    "data"
  ],
  "properties": {
    "code": {
      "$id": "#/properties/code",
      "type": "string",
      "title": "The Code Schema",
      "default": "",
      "enm": [
        "200"
      ],
      "pattern": "\\d"
    },
    "msg": {
      "$id": "#/properties/msg",
      "type": "string",
      "title": "The Msg Schema",
      "default": "",
      "enum": [
        "customer exists"
      ],
      "pattern": "^(.*)$"
    },
    "data": {
      "$id": "#/properties/data",
      "type": ["string","null"],
      "title": "The Data Schema",
      "pattern": "\\d"
    }
  }
}