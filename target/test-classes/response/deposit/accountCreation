 {
   "definitions": {},
   "$schema": "http://json-schema.org/draft-07/schema#",
   "$id": "http://example.com/root.json",
   "type": "object",
   "title": "The Root Schema",
   "required": [
     "code",
     "msg",
     "data"
   ],
   "properties": {
     "code": {
       "$id": "#/properties/code",
       "type": "string",
       "title": "The Code Schema",
       "default": "",
       "examples": [
         "1"
       ],
       "pattern": "^(.*)$"
     },
     "msg": {
       "$id": "#/properties/msg",
       "type": "string",
       "title": "The Msg Schema",
       "default": "",
       "examples": [
         "Created"
       ],
       "pattern": "^(.*)$"
     },
     "data": {
       "$id": "#/properties/data",
       "type": "string",
       "title": "The Data Schema",
       "default": "",
       "examples": [
         "HK790001001000066888001"
       ],
       "pattern": "^(.*)$"
     }
   }
 }