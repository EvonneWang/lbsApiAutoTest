      "default": "",
      "examples": [
        "200"
      ],
      "pattern": "^-?\\d+(\\.\\d+)?$"
    },
    "msg": {
      "$id": "/properties/msg",
      "type": "string",
      "title": "The Msg Schema",
      "default": "",
      "examples": [
        "Validation Succeed"
      ],
      "pattern": "^(.*)$"
    },
    "data": {
      "$id": "#/properties/data",
      "type": "null",
      "title": "The Data Schema",
      "default": null,
