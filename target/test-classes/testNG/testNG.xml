<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SuiteName" verbose="1" parallel="tests">
    <test name="TestName">
        <groups>
            <run>
                <include name="all"></include>
            </run>
        </groups>
        <classes>
            <class name="api.CreditCardApiTest"/>
